{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "project_column",

  "type": "object",
  "required": ["action", "project_column", "repository", "sender"],
  "properties": {
    "action": {
      "type": "string",
      "enum": ["created", "edited", "moved", "deleted"]
    },
    "project_column": {
      "type": "object",
      "required": ["url", "project_url", "cards_url", "id", "node_id", "name", "created_at", "updated_at"],
      "properties": {
        "url": {
          "type": "string"
        },
        "project_url": {
          "type": "string"
        },
        "cards_url": {
          "type": "string"
        },
        "id": {
          "type": "integer"
        },
        "node_id": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "created_at": {
          "type": "string"
        },
        "updated_at": {
          "type": "string"
        }
      },
      "additionalProperties": true
    },
    "repository": {
      "$ref": "$repository"
    },
    "sender": {
      "$ref": "$sender"
    },
    "installation": {
      "$ref": "$installation"
    },
    "organization": {
      "$ref": "$organization"
    }
  },
  "additionalProperties": true
}
