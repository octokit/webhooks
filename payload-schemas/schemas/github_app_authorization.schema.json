{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "github_app_authorization",

  "type": "object",
  "required": ["action", "sender"],
  "properties": {
    "action": {
      "type": "string",
      "enum": ["revoked"]
    },
    "sender": { "$ref": "$user" }
  },
  "additionalProperties": false
}
