{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "common/organization.schema.json",
  "type": "object",
  "required": [
    "login",
    "id",
    "node_id",
    "url",
    "repos_url",
    "events_url",
    "hooks_url",
    "issues_url",
    "members_url",
    "public_members_url",
    "avatar_url",
    "description"
  ],
  "properties": {
    "login": { "type": "string" },
    "id": { "type": "integer" },
    "node_id": { "type": "string" },
    "url": { "type": "string", "format": "uri" },
    "html_url": { "type": "string", "format": "uri" },
    "repos_url": { "type": "string", "format": "uri" },
    "events_url": { "type": "string", "format": "uri" },
    "hooks_url": { "type": "string", "format": "uri" },
    "issues_url": { "type": "string", "format": "uri" },
    "members_url": { "type": "string", "format": "uri-template" },
    "public_members_url": { "type": "string", "format": "uri-template" },
    "avatar_url": { "type": "string", "format": "uri" },
    "description": { "type": ["string", "null"] }
  },
  "additionalProperties": false,
  "title": "Organization"
}
