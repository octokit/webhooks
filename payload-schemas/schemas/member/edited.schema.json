  "properties": {
    "action": {
      "type": "string",
      "enum": ["edited"]
    },
    "member": {
      "$ref": "common/user.schema.json"
    },
    "changes": {
      "type": "object"
    },
    "repository": {
      "$ref": "common/repository.schema.json"
    },
    "installation": {
      "$ref": "common/installation.schema.json"
    },
    "sender": {
      "$ref": "common/user.schema.json"
    }
  },
  "additionalProperties": false,
  "title": "member edited event"
}
