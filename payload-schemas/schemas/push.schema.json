{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "push",

  "type": "object",
  "required": [
    "ref",
    "before",
    "after",
    "created",
    "deleted",
    "forced",
    "base_ref",
    "compare",
    "commits",
    "head_commit",
    "repository",
    "pusher",
    "sender"
  ],
  "properties": {
    "ref": {
      "type": "string"
    },
    "before": {
      "type": "string"
    },
    "after": {
      "type": "string"
    },
    "created": {
      "type": "boolean"
    },
    "deleted": {
      "type": "boolean"
    },
    "forced": {
      "type": "boolean"
    },
    "base_ref": {
      "type": "null"
    },
    "compare": {
      "type": "string"
    },
    "commits": {
      "type": "array",
      "additionalItems": false,
      "items": {}
    },
    "head_commit": {
      "type": "null"
    },
    "repository": {
      "$ref": "$repository"
    },
    "sender": {
      "$ref": "$sender"
    },
    "installation": {
      "$ref": "$installation"
    },
    "organization": {
      "$ref": "$organization"
    },
    "pusher": {
      "type": "object",
      "required": ["name", "email"],
      "properties": {
        "name": {
          "type": "string"
        },
        "email": {
          "type": "string"
        }
      },
      "additionalProperties": true
    }
  },
  "additionalProperties": true
}
