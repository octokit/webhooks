{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "project_card$created",
  "type": "object",
  "required": ["action", "project_card", "repository", "sender"],
  "properties": {
    "action": {
      "type": "string",
      "enum": ["created"]
    },
    "project_card": {
      "type": "object",
      "required": [
        "url",
        "project_url",
        "column_url",
        "column_id",
        "id",
        "node_id",
        "note",
        "archived",
        "creator",
        "created_at",
        "updated_at"
      ],
      "properties": {
        "url": {
          "type": "string"
        },
        "project_url": {
          "type": "string"
        },
        "column_url": {
          "type": "string"
        },
        "column_id": {
          "type": "integer"
        },
        "id": {
          "type": "integer"
        },
        "node_id": {
          "type": "string"
        },
        "note": {
          "type": "string"
        },
        "archived": {
          "type": "boolean"
        },
        "creator": {
          "$ref": "common/user.schema.json"
        },
        "created_at": {
          "type": "string"
        },
        "updated_at": {
          "type": "string"
        }
      },
      "additionalProperties": false
    },
    "repository": {
      "$ref": "common/repository.schema.json"
    },
    "sender": {
      "$ref": "common/user.schema.json"
    },
    "organization": {
      "$ref": "common/organization.schema.json"
    },
    "installation": {
      "$ref": "common/installation.schema.json"
    }
  },
  "additionalProperties": false,
  "title": "project_card created event"
}
