{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "ping",

  "type": "object",
  "required": ["zen", "hook_id", "hook", "repository", "sender"],
  "properties": {
    "zen": {
      "type": "string"
    },
    "hook_id": {
      "type": "integer"
    },
    "hook": {
      "type": "object",
      "required": [
        "type",
        "id",
        "name",
        "active",
        "events",
        "config",
        "updated_at",
        "created_at",
        "url",
        "test_url",
        "ping_url",
        "last_response"
      ],
      "properties": {
        "type": {
          "type": "string"
        },
        "id": {
          "type": "integer"
        },
        "name": {
          "type": "string"
        },
        "active": {
          "type": "boolean"
        },
        "events": {
          "type": "array",
          "additionalItems": false,
          "items": {
            "anyOf": [
              {
                "type": "string"
              }
            ]
          }
        },
        "config": {
          "type": "object",
          "required": ["content_type", "url", "insecure_ssl"],
          "properties": {
            "content_type": {
              "type": "string"
            },
            "url": {
              "type": "string"
            },
            "insecure_ssl": {
              "type": "string"
            }
          },
          "additionalProperties": true
        },
        "updated_at": {
          "type": "string"
        },
        "created_at": {
          "type": "string"
        },
        "url": {
          "type": "string"
        },
        "test_url": {
          "type": "string"
        },
        "ping_url": {
          "type": "string"
        },
        "last_response": {
          "type": "object",
          "required": ["code", "status", "message"],
          "properties": {
            "code": {
              "type": "null"
            },
            "status": {
              "type": "string"
            },
            "message": {
              "type": "null"
            }
          },
          "additionalProperties": true
        }
      },
      "additionalProperties": true
    },
    "repository": {
      "$ref": "$repository"
    },
    "sender": {
      "$ref": "$sender"
    },
    "organization": {
      "$ref": "$organization"
    }
  },
  "additionalProperties": true
}
