{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "repository_dispatch",

  "type": "object",
  "required": [
    "action",
    "branch",
    "client_payload",
    "repository",
    "organization",
    "sender",
    "installation"
  ],
  "properties": {
    "action": {
      "type": "string"
    },
    "branch": {
      "type": "string"
    },
    "client_payload": {
      "type": "object",
      "properties": {},
      "additionalProperties": true
    },
    "repository": {
      "$ref": "$repository"
    },
    "sender": {
      "$ref": "$user"
    },
    "installation": {
      "$ref": "$installation"
    },
    "organization": {
      "$ref": "$organization"
    }
  },
  "additionalProperties": false
}
