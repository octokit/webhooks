{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "milestone$deleted",
  "type": "object",
  "required": ["action", "milestone", "repository", "sender"],
  "properties": {
    "action": {
      "type": "string",
      "enum": ["deleted"]
    },
    "milestone": {
      "type": "object",
      "required": [
        "url",
        "html_url",
        "labels_url",
        "id",
        "node_id",
        "number",
        "title",
        "description",
        "creator",
        "open_issues",
        "closed_issues",
        "state",
        "created_at",
        "updated_at",
        "due_on",
        "closed_at"
      ],
      "properties": {
        "url": {
          "type": "string"
        },
        "html_url": {
          "type": "string"
        },
        "labels_url": {
          "type": "string"
        },
        "id": {
          "type": "integer"
        },
        "node_id": {
          "type": "string"
        },
        "number": {
          "type": "integer"
        },
        "title": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "creator": {
          "$ref": "common/user.schema.json"
        },
        "open_issues": {
          "type": "integer"
        },
        "closed_issues": {
          "type": "integer"
        },
        "state": {
          "type": "string"
        },
        "created_at": {
          "type": "string"
        },
        "updated_at": {
          "type": "string"
        },
        "due_on": {
          "type": "string"
        },
        "closed_at": {
          "oneOf": [
            {
              "type": "null"
            },
            {
              "type": "string"
            }
          ]
        }
      },
      "additionalProperties": false
    },
    "repository": {
      "$ref": "common/repository.schema.json"
    },
    "sender": {
      "$ref": "common/user.schema.json"
    },
    "installation": {
      "$ref": "common/installation.schema.json"
    },
    "organization": {
      "$ref": "common/organization.schema.json"
    }
  },
  "additionalProperties": false,
  "title": "milestone deleted event"
}
