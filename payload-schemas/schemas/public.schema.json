{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "public",

  "type": "object",
  "required": ["repository", "sender"],
  "properties": {
    "repository": {
      "$ref": "$repository"
    },
    "sender": {
      "$ref": "$user"
    },
    "installation": {
      "$ref": "$installation"
    },
    "organization": {
      "$ref": "$organization"
    }
  },
  "additionalProperties": false
}
