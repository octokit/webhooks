{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "page_build",

  "type": "object",
  "required": ["id", "build", "repository", "sender"],
  "properties": {
    "id": {
      "type": "integer"
    },
    "build": {
      "type": "object",
      "required": [
        "url",
        "status",
        "error",
        "pusher",
        "commit",
        "duration",
        "created_at",
        "updated_at"
      ],
      "properties": {
        "url": {
          "type": "string"
        },
        "status": {
          "type": "string"
        },
        "error": {
          "type": "object",
          "required": ["message"],
          "properties": {
            "message": {
              "type": "null"
            }
          },
          "additionalProperties": false
        },
        "pusher": { "$ref": "common/user.schema.json" },
        "commit": {
          "type": "string"
        },
        "duration": {
          "type": "integer"
        },
        "created_at": {
          "type": "string"
        },
        "updated_at": {
          "type": "string"
        }
      },
      "additionalProperties": false
    },
    "repository": {
      "$ref": "common/repository.schema.json"
    },
    "sender": {
      "$ref": "common/user.schema.json"
    },
    "installation": {
      "$ref": "common/installation.schema.json"
    },
    "organization": {
      "$ref": "common/organization.schema.json"
    }
  },
  "additionalProperties": false
}
