{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "pull_request_review$submitted",
  "type": "object",
  "required": ["action", "review", "pull_request", "repository", "sender"],
  "properties": {
    "action": { "type": "string", "enum": ["submitted"] },
    "review": { "$ref": "common/pull-request-review.schema.json" },
    "pull_request": { "$ref": "common/simple-pull-request.schema.json" },
    "repository": { "$ref": "common/repository.schema.json" },
    "installation": { "$ref": "common/installation-lite.schema.json" },
    "organization": { "$ref": "common/organization.schema.json" },
    "sender": { "$ref": "common/user.schema.json" }
  },
  "additionalProperties": false,
  "title": "pull_request_review submitted event"
}
